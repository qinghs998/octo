{
  "name": "gt-react-scaffold",
  "version": "1.7.0",
  "description": "react gt scaffold",
  "main": "webpack.config.js",
  "scripts": {
    "test": "ava",
    "lint": "eslint ./src",
    "release": "standard-version && git push --follow-tag",
    "dev": "NODE_ENV=development node ./scripts/dev.js",
    "beta": "NODE_ENV=beta npm run build",
    "production": "NODE_ENV=production npm run build",
    "build": "npm run build:clean && npm run build:webpack",
    "build:clean": "rm -rf ./release && mkdir ./release",
    "build:webpack": "node ./scripts/build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vivaxy/gt-react-scaffold.git"
  },
  "keywords": [
    "react",
    "gt-react-scaffold",
    "scaffold",
    "gt"
  ],
  "author": "vivaxy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vivaxy/gt-react-scaffold/issues"
  },
  "homepage": "https://github.com/vivaxy/gt-react-scaffold#readme",
  "devDependencies": {
    "autoprefixer": "^6.7.0",
    "ava": "^0.18.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.0",
    "css-loader": "^0.24.0",
    "eslint": "^3.18.0",
    "eslint-config-vivaxy": "^0.1.1",
    "eslint-loader": "^1.7.1",
    "execa": "^0.6.0",
    "file-loader": "^0.9.0",
    "glob": "^7.1.1",
    "html-webpack-plugin": "^2.28.0",
    "ip": "^1.1.4",
    "isomorphic-fetch": "^2.2.1",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "listr": "^0.8.0",
    "log-update": "^1.0.2",
    "numeral": "^2.0.4",
    "postcss-loader": "^1.2.2",
    "prop-types": "^15.5.7",
    "react-addons-css-transition-group": "^15.3.2",
    "react-dev-utils": "^0.5.2",
    "react-hot-loader": "next",
    "react-pianist": "^0.0.12",
    "react-redux": "^5.0.4",
    "react-router": "3",
    "react-router-redux": "^4.0.6",
    "redux-devtools": "^3.3.1",
    "standard-version": "^3.0.0",
    "style-loader": "^0.13.1",
    "tiny-cookie": "^0.5.5",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-visualizer-plugin": "^0.1.5"
  },
  "browserslist": [
    "> 1%",
    "ie >= 9"
  ],
  "ava": {
    "files": [
      "test/*.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit",
    "verbose": true
  },
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  }
}
